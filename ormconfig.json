{
  "name": "default",
  "type": "postgres",
  "url": process.env.DATABASE_URL,
  "synchronize": false,
  "logging": true,
  "ssl": true,
  "entities": [
    "./src/models/*.ts"
  ],
  "migrations" : [
    "./src/database/migrations/*.ts"
  ],
  "cli":{
    "migrationsDir" : "./src/database/migrations"
}
}
