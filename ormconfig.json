{
  "name": "default",
  "type": "postgres",
  "url": "postgres://username:password@hostname:5432/databasename",
  "synchronize": false,
  "logging": true,
  "ssl": true,
  "entities": [
    "./src/models/*.ts"
  ],
  "migrations" : [
    "./src/database/migrations/*.ts"
  ],
  "cli":{
    "migrationsDir" : "./src/database/migrations"
}
}
